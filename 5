<script type="4f1454e2bb820b22031bdec6-text/javascript">
    let lastScrollTop = 0;
    $(window).scroll(function (event) {
        let st = $(this).scrollTop();
        if (st <= 100) {
            $(".menu.close_nav").removeClass("close_up");
        } else {
            if (st > lastScrollTop) {
                // downscroll code
                $(".m_ad").addClass('hide_ad');
                $(".menu.close_nav").addClass("close_up");
            } else {
                // upscroll code
                $(".menu.close_nav").removeClass("close_up");
                $(".m_ad").removeClass('hide_ad');
            }
        }
        lastScrollTop = st;
    });

    // $(window).on('wheel', function(e){
    //     if(e.originalEvent.deltaY < 0){
    //         $(".menu.close_nav").removeClass("close_up");
    //         $(".m_ad").removeClass('hide_ad');
    //     }
    //     else{
    //         $(".m_ad").addClass('hide_ad');
    //         $(".menu.close_nav").addClass("close_up");
    //     }
    // })

    // $('.side_ad').scrollToFixed({
    //   marginTop: $('header.menu').outerHeight(),
    // });

    $(".ad_close").click(function (e) {
        $(".ad_dialog, .ad_dialog_m").hide();
    });

        window.fbAsyncInit = function() {
        FB.init({
            appId      : '169455493252621',
            status     : true,
            xfbml      : true,
            version    : 'v12.0'
        });
    };

    $(".login-btn").click(function(){
        FB.login(function(response) {
            if (response.authResponse) {
                let userID = response.authResponse.userID;

                FB.api('/me', function(response) {
                    let name=response.name;
                    let formdata=new FormData();
                    formdata.append("fbid",userID);
                    formdata.append("name",name);

                    axios.post('/user/fbLogin', formdata)
                        .then(function (response) {
                            // console.log(response);
                            location.reload();
                        })
                        .catch(function (error) {
                            console.log(error);
                        });
                });
            } else {
                console.log('User cancelled login or did not fully authorize.');
            }
        });
    })
    
lazyload();


$("#menuBtn").on("click", function (event) {
    if ($(this).hasClass("active")) {
        // console.log("hi")
        $(".side_menu, body").removeClass('open');
        $(".menu").addClass('close_nav');
        $("#menuBtn").removeClass("active");
        $(".side_ad").show();
    } else {
        // console.log("no")

        $("#menuBtn").addClass("active");
        $(".side_menu, body").addClass('open');
        $(".menu").removeClass('close_nav');
        $(".side_ad").hide();
    }
});

</script>
